import shutil
import datetime
import os

def backup_file(src,dest):
    today=datetime.date.today()
    backup_file_name=os.path.join(dest,f"bacup_file_{today}.tar.gz")
    shutil.make_archive(backup_file_name,'tar.gz',src)
src="C:\Users\qasim\OneDrive - Higher Education Commission\Documents\devOps_journey"
dest="C:\Users\qasim\OneDrive - Higher Education Commission\Documents\devOps_journey\backups"

backup_file(src,dest)